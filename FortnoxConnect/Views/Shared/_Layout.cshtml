<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Fortnox Connect POC</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
</head>
<body>
    <div class="header">
        <div class="container">
            <h1>Fortnox Connect POC</h1>
            <p>ASP.NET 4.7 OAuth2 Authentication Demo</p>
        </div>
    </div>

    <div class="container">
        @RenderBody()
    </div>

    <div class="footer">
        <div class="container">
            <p>&copy; @DateTime.Now.Year - Fortnox Connect POC</p>
        </div>
    </div>
</body>
</html>
